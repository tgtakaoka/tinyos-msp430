# -*- mode: makefile-gmake; mode: flyspell-prog; -*-

TARGET = msp430f2131

# Disable MSP430 hardware multiply because it makes MSPGCC die
PFLAGS += -mmpy=none -mdisable-watchdog

MSP_BSL_FLAGS = --invert-reset
$(call TOSMake_include,msp/bsl.extra)
$(call TOSMake_include_platform,msp430f2131)
$(call TOSMake_include_make_platform,msp)

msp430f2131: $(BUILD_DEPS)
	@:

